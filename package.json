{
  "name": "dgclr",
  "version": "0.4.10",
  "description": "Disc Golf Course Lists and Results",
  "main": "server.js",
  "author": "Janne Saarela",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "if [ $RUN_BUILD ]; then yarn build; fi",
    "prebuild": "rimraf ./dist/*",
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "validate": "npm ls",
    "flow": "flow",
    "flow-typed-install": "flow-typed install",
    "test": "yarn lint && yarn jest && yarn flow",
    "jest": "NODE_ENV=test jest",
    "patch-version": "mversion patch",
    "minor-version": "mversion minor",
    "major-version": "mversion major",
    "prepublish": "if [ $RUN_SNYK ]; then yarn snyk-protect; fi",
    "snyk-protect": "snyk protect"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0 ",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@emotion/babel-plugin": "^11.7.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-define": "^2.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "flow-bin": "^0.125.1",
    "flow-typed": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^27.4.5",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.1.2",
    "mversion": "^2.0.1",
    "nodemon": "^2.0.15",
    "prettier": "2.2.1",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1"
