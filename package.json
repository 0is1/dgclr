    "apollo-link-http": "^1.5.17",
    "body-parser": "^1.19.1",
    "dotenv": "^10.0.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-favicon-short-circuit": "^1.2.0",
    "express-session": "^1.17.2",
    "forcedomain": "^2.2.11",
    "graphql": "^15.0.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.6",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "next": "^10.2.3",
    "next-i18next": "^4.5.0",
