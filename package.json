  "lint-staged": {
    "*.{js,json,css}": [
      "eslint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "^8.16.0 ||Â ^10.16.0",
    "yarn": "^1.13.0"
  },
