    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^5.0.7",
    "rebass": "^2.1.0",
    "recompose": "^0.29.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
