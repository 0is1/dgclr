    "react-apollo": "^3.1.5",
    "react-compound-slider": "^3.3.1",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-select": "^4",
    "react-spinners": "^0.11.0",
    "react-switch": "^6.0.0",
    "rebass": "2.3.4",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "rollbar": "^2.24.0",
    "serve-favicon": "^2.5.0",
    "sitemap": "^7.0.0",
    "snyk": "^1.794.0",
    "string.prototype.matchall": "^4.0.6",
