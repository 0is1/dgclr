{
  "name": "dgclr",
  "version": "0.4.5",
  "description": "Disc Golf Course Lists and Results",
  "main": "server.js",
  "author": "Janne Saarela",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "if [ $RUN_BUILD ]; then yarn build; fi",
    "prebuild": "rimraf ./dist/*",
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "validate": "npm ls",
    "flow": "flow",
    "flow-typed-install": "flow-typed install",
    "test": "yarn lint && yarn jest && yarn flow",
    "jest": "NODE_ENV=test jest",
    "patch-version": "mversion patch",
    "minor-version": "mversion minor",
    "major-version": "mversion major",
    "prepublish": "if [ $RUN_SNYK ]; then yarn snyk-protect; fi",
    "snyk-protect": "snyk protect"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-define": "^2.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "flow-bin": "^0.125.1",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "mversion": "^1.13.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "@react-google-maps/api": "^1.9.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-http": "^1.5.17",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-favicon-short-circuit": "^1.2.0",
    "express-session": "^1.17.1",
    "forcedomain": "^2.0.0",
    "graphql": "^15.0.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "next": "^9.4.1",
    "next-i18next": "^4.4.2",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "pm2": "^4.4.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-compound-slider": "^2.5.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-html-parser": "^2.0.2",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.8.3",
    "react-switch": "^5.0.1",
    "rebass": "2.3.4",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^1.1.3",
    "rollbar": "^2.16.2",
    "serve-favicon": "^2.5.0",
    "sitemap": "^6.1.4",
    "snyk": "^1.321.0",
    "string.prototype.matchall": "^4.0.2",
    "styled-components": "^5.1.0",
    "updeep": "^1.2.0",
    "uuid4": "^1.1.4"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "eslint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "^12",
    "yarn": "^1.22.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/jest/jest.setup.js",
      "<rootDir>/jest/jest.mock.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-commit": "lint-staged"
    }
  },
  "dependenciesComments": {
    "rebass": "Locked to 2.3.4 for now. Version >=3.0.0 has many breaking changes SEE: https://github.com/rebassjs/rebass/releases/tag/v3.0.0."
  },
  "snyk": true
}
